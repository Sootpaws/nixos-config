theme: with theme.colors; ''

color-link default             "${secondary.strong}"
color-link selection           "${primary.strong},${secondary.strong}"

color-link statusline          "${secondary.weak},${primary.medium}"
color-link tabbar              "${secondary.weak},${primary.medium}"
color-link divider             "${primary.medium},${secondary.weak}"
